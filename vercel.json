{
  "version": 2,
  "builds": [
    {
      "src": "dist/Interns-R-Ventory-WebUI", // Replace with the correct path to your Angular app's build output
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
